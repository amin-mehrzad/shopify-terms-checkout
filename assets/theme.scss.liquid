    font-family: $font-stack-accent;
    font-weight: $font-weight-accent;
    font-style: $font-style-accent;
    @include size-header(24px);
  }

  @include at-query ( $max, $largish ) {
    margin-top: 10px;
  }

  .cart__discount {
    span { @include size-header(14px); }
  }
}

.cart__mini-labels {
  display: block;
  margin: ($gutter / 3) 0;
  @include size-body(12px);

  @include at-query ($min, $large) {
    display: none;
  }
}

.cart__remove {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 32px;
  height: 32px;

  @include at-query ($max, $largish) {
    position: absolute;
    top: 75px;
    left: 0;
    margin-top: -16px;
  }
}

.cart__update {
  display: none;
  .no-js &{
    display: inline-block;
  }
}

.cart-title {
  @include size-header(48px);
  @include at-query ($max, $largish) {
    @include size-header(28px);
    text-align: center;
    padding-bottom: 0px;
  }
}

.cart__row {
  textarea {
    border: 2px solid $colorBorder;
  }
}

.template-cart {
  .custom-icon-x {
    color: $colorTextBody;
    -webkit-text-stroke: .5px;
  }
  .js-qty__num {
    border: none !important;
    text-align: center;
  }
}

.shipping-note {
  display: block;
  padding-top: 10px;
  padding-bottom: 10px;

  @include at-query ($max, $medium) {
    text-align: center;
  }
}

.cart__header-labels {border-bottom: 2px solid $colorBorder;}
//Currencies

#currencies {
  display: -moz-inline-stack;
  display: inline-block;
  zoom: 1;
  *display: inline;
  color: #bbbbbb;
  font-size: 0; /* to eliminate space between buttons */
  line-height: 1.5;
  cursor: pointer;

  span {
    display: -moz-inline-stack;
    display: inline-block;
    zoom: 1;
    *display: inline;
    padding: 5px 10px;
    border: 1px solid #D6D6D6;
    background: none #F6F6F6;
    font-size: 13px;
  }

  .left  {
    -webkit-border-top-left-radius: 7px;
    -webkit-border-bottom-left-radius: 7px;
    -moz-border-radius-topleft: 7px;
    -moz-border-radius-bottomleft: 7px;
    border-top-left-radius: 7px;
    border-bottom-left-radius: 7px;
  }
  .right {
    -webkit-border-top-right-radius: 7px;
    -webkit-border-bottom-right-radius: 7px;
    -moz-border-radius-topright: 7px;
    -moz-border-radius-bottomright: 7px;
    border-top-right-radius: 7px;
    border-bottom-right-radius: 7px;
    border-left: none;
  }
  .selected {
    font-weight: $font-weight-normal;
    color: $colorTextBody;
    background: #eee;
  }
}

.currency-picker {
  display: inline-block;
  position: relative;
  margin: 0 0 0 20px;
  vertical-align: middle;
  border-width: 1px;

  &::after{
    font-family: "custom-icons" !important;
    font-style: normal !important;
    font-weight: normal !important;
    font-variant: normal !important;
    text-transform: none !important;
    speak: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    content: "\63";
    margin-top: -7px;
    position: absolute;
    right: 10px;
    top: 50%;
    z-index: 1;
    font-size: 12px;
    display: block;
    color: $dropdownTextColor;
    pointer-events: none;
  }

  select {
    background-color: $dropdownBg;
    color: $dropdownTextColor;
  }

  .drawer & { margin-left: 0; }
}

.template-cart {
  h3 {
    margin-bottom: 0px;
  }
}

/*============================================================================
  #Page Template
==============================================================================*/

.template-page p {line-height:2;}

.page-title {padding-bottom:25px;}

/*============================================================================
  #Footer
==============================================================================*/

.site-footer {
  color: $colorTextBody;
  padding-top:100px;
  padding-bottom:100px;
  clear:both;

  .h5 {
    p {
      margin: 0;
    }
  }


  .grid--full {
    .grid__item {
      padding-left: 25px;
      padding-right: 25px;
    }

    @include at-query ($min, 1600px) {
      max-width: 1400px;
      margin-left: auto;
      margin-right: auto;
      position: relative;
    }
  }
}

.footer-logo {
  position: relative;
  display: inline-block;
  color: $colorTextHeading;
  font-family: $font-stack-heading;
  font-weight: $font-weight-heading;
  font-style: $font-style-heading;
  margin-bottom: 25px;
  max-width: 100%;

  img {
    display: block;
    vertical-align: top;
    max-width: 100%;
    height: auto;
  }

  .placeholder-svg {
    max-width: 100%;
    position: relative;
    overflow: hidden;
  }

  a { color: $colorTextLight; }
  p { margin:0px;}

  @include at-query ( $max, $largish ) {
    margin-left: auto;
    margin-right:auto;
  }
}

.shopify-link {
  margin-top: 100px;
  a { @include size-body(12px); }

  @include at-query ( $max, $medium ) {
    margin-top: 25px;
  }
}

.payment-icon-list {
  margin: 0;

  & > li {
    @include size-body(22px);
    display: inline-block;
    list-style: outside none none;
    margin: 5px 10px 0 0;

    &:last-child {
      margin-right: 0;
    }
  }

  .payment-icon {
    display: block;
    width: 32px;
  }
}

.footer-social-icons {
  margin-left: auto;
  margin-right: auto;
  display: block;

  & > li {
    list-style: outside none none;
    margin-bottom: 10px;
  }

  i { vertical-align: middle; }
  a { @include size-body(14px); }
}

.footer-social-icon {
  color:$colorTextLight;
  padding-right: 10px;
  float:left;
}

.blog {
  @include size-body(18px);
}

.article-title {
  @include size-body(14px);
  text-transform: capitalize;
}

.article-excerpt {
  margin-top: 0px;
  a {
    @include size-body(12px);
    color:$colorTextLight;
  }
}

.comment{
  .rte {margin-top:-10px;}
}

.light-gray a {color:$colorTextLight;}
.light-gray a:hover { color: $colorTextBody; }

.footer-text p:first-child { margin-top: 0; }
.footer-text-col {margin-top:0px !important;}

.social-sharing-wrapper i {
  @include size-body(22px);
  color: $colorTextLight;
  padding-right:20px;
}

/*============================================================================
  #Top bar
==============================================================================*/

.top-bar {
  position: relative;
  width: 100%;
  padding-top: 5px;
  padding-bottom: 5px;
  text-align: center;

  a {
    display: block;
    color: inherit;
  }
}
.top-bar__slide {
  width: 100%;
}

/*============================================================================
  #Popup
==============================================================================*/

.popup {
  position: relative;
  width: 100%;
  padding: 25px;
  margin: auto;

  &.popup--medium {
    width: 33.33%;
    min-width: 500px;
  }

  &.popup--align-left {
    float: left;
  }

  &.popup--align-right {
    float: right;
  }

  h2 {
    @include size-header(24px);
  }

  .newsletter {
    margin-top: 25px;
    padding-top: 0px;
    padding-bottom: 0px;
  }

  #mc-embedded-subscribe-form{
    display: table;
    margin-left: auto;
    margin-right: auto;
    position: relative;
    margin-top:25px;
  }

}

.popup--text-light {
  color: #fff;

  h2,
  #mail,
  #subscribe,
  .custom-icon-long-arrow,
  p,
  a,
  a::before,
  input {
    color: inherit;
  }

  #mail {
    border-color: #fff;
    background-color: transparent;
  }

  ::-webkit-input-placeholder { color: #fff; }
  ::-moz-placeholder { color: #fff; }
  :-ms-input-placeholder { color: #fff; }
  :-moz-placeholder { color: #fff; }

  #subscribe {
    background-color: transparent;
    border-color: #fff;

    &:hover {
      background: #fff;
      .custom-icon-long-arrow {
        color: $colorPrimary;
      }
    }
  }
}

.popup__background {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;

  &::after {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
  }
}

.popup__container {
  max-width: 500px;
  margin: auto;
  position: relative;

  @include at-query ( $max, $medium ) {
    width:100%;
    padding: 50px 25px;

    .newsletter {
      max-width: 75%;
    }
  }
}

.blocker {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  overflow: auto;
  z-index: 9999;
  box-sizing: border-box;
  text-align: center;
  transition: all 0.3s cubic-bezier(.25,.8,.25,1);

  @include at-query ( $max, $medium ) {
    display: none;
  }

  @include at-query ( $max, $small ) {
    padding: 50px 10px;

    .newsletter{
      max-width: auto;
    }
  }
}

.close-modal{
  position: absolute;
  top: 0px;
  right: 0px;
  padding:25px;

  &::before{
    font-family: 'custom-icons';
    content: "\69";
    text-decoration: none;
    speak: none;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    vertical-align: middle;
    font-size: 20px;
    color: $colorTextHeading;
  }
}

