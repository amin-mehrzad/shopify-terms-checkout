==============================================================================*/
$viewportIncrement: 1px;

$small: 480px;
$medium: 767px;
$large: 768px;
$largish: 991px;
$largest: 1024px;
$widescreen: 1400px;

$postSmall: $small + $viewportIncrement;
$preLargest: $largest - $viewportIncrement;

/*================ The following are dependencies of csswizardry grid ================*/
$breakpoints: (
  'small' '(max-width: #{$small})',
  'medium' '(min-width: #{$postSmall}) and (max-width: #{$medium})',
  'medium-down' '(max-width: #{$medium})',
  'large' '(min-width: #{$large})',
  'largest' '(min-width: #{$largest})',
  'largest-down' '(max-width: #{$preLargest})',
  'widescreen' '(min-width: #{$widescreen})'
);
$breakpoint-has-widths: ('small', 'medium', 'medium-down', 'large', 'largest-down', 'largest', 'widescreen');
$breakpoint-has-push:  ('medium', 'medium-down', 'large', 'widescreen');
$breakpoint-has-pull:  ('medium', 'medium-down', 'large', 'widescreen');

