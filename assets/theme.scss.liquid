/*================ Product Features ================*/
.product-features {
  padding-top: 50px;
  padding-bottom: 50px;
  .btn {
    margin-bottom: 25px;
  }
}

.placeholder-svg {
  display: block;
  width: 100%;
  height: 100%;
  max-width: 100%;
  max-height: 100%;
  fill: #b4b6b9;
}

.placeholder-svg-filled {
  @extend .placeholder-svg;
  background-color: darken($colorBody,5%);
}

.product-feature {
  margin: 0 auto 50px;

  @include at-query($min, $large) {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 100px;
  }

  &:last-child {
    margin-bottom: 0;
  }
}

.product-feature--stacked {
  max-width: 1100px;
  margin-left: auto;
  margin-right: auto;
}

.product-feature--overlapping {
  max-width: 1300px;

  @include at-query($min, $large) {
    .product-feature__images {
      width: 50%;
    }

    .product-feature__text {
      width: 50%;
    }

    &.product-feature--odd {
      .product-feature__text {
        padding: 0 0 0 40px;
      }

      .product-feature__image--primary {
        margin: 0 auto 0 0;
      }
      .product-feature__image--secondary {
        margin-right: 0;
        margin-left: auto;
      }
    }

    &.product-feature--even {
      .product-feature__text {
        padding: 0 40px 0 0;
      }

      .product-feature__image--primary {
        margin: 0 0 0 auto;
      }
      .product-feature__image--secondary {
        margin-right: auto;
        margin-left: 0;
      }
    }

    .product-feature__image {
      width: 62%;
    }

    .product-feature__image--primary {
      position: relative;
    }

    .product-feature__image--secondary {
      margin-top: calc(100% * 0.12 * -1);
    }
  }

  @include at-query($min, $largest) {
    .product-feature__images {
      width: calc(100% - 515px);
      max-width: 50%;
    }

    .product-feature__text {
      width: 440px;
      min-width: calc(50% - 115px);
      padding: 0 !important;
    }
  }

  @include at-query($max, $medium) {
    &.product-feature--odd {
      .product-feature__image--primary {
        margin: 0 auto 0 0;
      }
      .product-feature__image--secondary {
        margin-left: auto;
        margin-right: 0;
      }
    }

    &.product-feature--even {
      .product-feature__image--primary {
        margin: 0 0 0 auto;
      }
      .product-feature__image--secondary {
        margin-right: auto;
        margin-left: 0;
      }
    }
  }
}

.product-feature--even {
  @include at-query($min, $large) {
    flex-direction: row-reverse;
  }

  .product-feature__text--overlapping {
    @include at-query ($min, $large) {
      position: relative;
      right: -12.5%;
    }
  }
}

.product-feature--odd {
  .product-feature__text--overlapping {
    @include at-query ($min, $large) {
      position: relative;
      left: -12.5%;
    }
  }
}

.product-feature--text-left {
  .product-feature__text {
    text-align: left;

    hr {
      margin-left: 0;
    }
  }
}

.product-feature__image {
  position: relative;
}
.product-feature__images {
  @include at-query($min, $large) {
    width: 50%;
  }
  @include at-query($max, $medium) {
    margin-left: -50px;
    margin-right: -50px;
    margin-top: -25px;

    .product-feature--overlapping & {
      margin-top: 50px;
    }

    .product-feature__image {
      width: 75%;

      .product-feature--stacked & {
        width: 100%;
        margin: 0;
      }
    }
    .product-feature__image--primary {
      position: relative;
    }

    .product-feature__image--secondary {
      margin-top: calc(100% * 0.25 * -1);
    }

    &.product-feature__images--mobile-single {
      .product-feature__image {
        width: 100%;
      }
    }
  }

  @include at-query($max, $postSmall) {
    margin-left: -25px;
    margin-right: -25px;
  }
}

.product-feature__images--mobile-single {
  @include at-query($max, $medium) {
    .product-feature__image--secondary {
      display: none !important;
    }
  }
}

.product-feature__image {
  img {
    display: block;
    width: 100%;
    height: auto;
  }
}


.product-feature__text {
  text-align: center;

  hr {
    display: block;
    margin: 15px auto;
    border: 0;
    width: 66px;
    height: 2px;
    background: $colorBorder;
  }

  h1 {
    margin: 25px 0;
    line-height: 1.06;

    @include at-query($max, $medium) {
      font-size: 22px;
      line-height: 1.42;
    }
  }

  p {
    line-height: 2;
    margin: 15px 0;
  }

  @include at-query($min, $largest) {
    padding: 0 55px;
  }
  @include at-query($min, $large) {
    width: 50%;
    padding: 0 25px;
  }
}

/*================ FAQ section ================*/
.faq-section,
.faq__content {
  margin: 50px 0;
}

.faq__header {
  text-align: center;
}
.faq__subheading {
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
}

.faq-list {
  list-style: none;
  max-width: 1000px;
  margin: 0 auto;
}

.faq-list__item {
  border-bottom: 1px solid $colorBorder;
}

.faq-list__item-question {
  position: relative;
  padding: 20px 40px 20px 0;
  cursor: pointer;

  .icon-toggle {
    position: absolute;
    top: 50%;
    right: 10px;
    width: 15px;
    height: 1px;

    &::before,
    &::after {
      content: '';
      position: absolute;
      left: 50%;
      top: 50%;
      width: 100%;
      height: 100%;
      background: #666;

      @include transition(transform, .5s, ease);
    }

    &::before {
      @include transform(unquote('translate(-50%, -50%) rotate(90deg)'));
    }

    &::after {
      @include transform(unquote('translate(-50%, -50%)'));
    }
  }

  &::before,
  &::after {
    content: '';
    position: absolute;
    left: 0;
    top: -12px;
    bottom: -12px;
    width: 0;
    opacity: .35;
    background: $colorBorder;
  }

  &::after {
    width: 100%;
    opacity: 0;
    -webkit-transition: width 0s linear 0s, opacity .5s ease;
    -moz-transition: width 0s linear 0s, opacity .5s ease;
    transition: width 0s linear 0s, opacity .5s ease;
  }
}

.is-active {
  .icon-toggle {
    &::before {
      @include transform(unquote('translate(-50%, -50%) rotate(0deg)'));
    }

    &::after {
      @include transform(unquote('translate(-50%, -50%) rotate(180deg)'));
    }
  }

  &::before {
    width: 100%;
    opacity: 0;
    @include transition(unquote('width 0s linear 0s, opacity .5s ease'));
  }

  &::after {
    width: 0;
    opacity: .35;
    transition: none;
  }
}

.faq-list__item-answer {
  overflow: hidden;
  max-height: 0;
  padding: 0;
  opacity: 0;
  @include transition(unquote('max-height .3s ease, padding .3s ease, opacity .3s ease'));

  .is-active & {
    max-height: 2000px;
    padding: 0 0 30px;
    opacity: 1;
  }
}

