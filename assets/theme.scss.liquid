    line-height: 30px;
  }

  .fallback-text {
    text-transform: capitalize;
  }
}

.social-icons li {
  margin: 0 ($gutter / 2) ($gutter / 2);
  vertical-align: middle;

  @include at-query ($min, $postSmall) {
    margin-left: 0;
  }

  .icon {
    font-size: 30px;
    line-height: 26px;
  }
}

/*============================================================================
  #Pagination
==============================================================================*/
.pagination {
  margin-bottom: 1em;
  text-align: center;

  > span {
    display: inline-block;
    line-height: 1;
  }

  a {
    display: block;
  }

  a,
  .page.current {
    padding: 8px;
  }
}

.pagination__loading {
  padding-top: 40px;
  display: inline-block;
  background: url({{ 'loading.svg' | asset_url }}) no-repeat top center;
  background-size: 36px 36px;
}

.dots--animated {
  span {
    animation-name: blink;
    animation-duration: 1.4s;
    animation-iteration-count: infinite;
    animation-fill-mode: both;

    &:nth-child(2) { animation-delay: .2s; }
    &:nth-child(3) { animation-delay: .4s; }
  }
}

@keyframes blink {
  0% {
    opacity: 0.2;
  }
  20% {
    opacity: 1;
  }
  100% {
    opacity: 0.2;
  }
}

/*============================================================================
  #Site Header
==============================================================================*/
.site-header {
  width: 100%;
  background-color: $colorHeaderBg;
  -webkit-overflow-scrolling: touch;
  will-change: initial !important;

  .grid--table {
    display: inline-table;
    table-layout: fixed;
    width: 100%;
    min-height: 60px;
    height: 100%;

    > .grid__item {
      float: none;
      display: table-cell;
      vertical-align: middle;
    }
  }

  .no-js & {position: relative !important;}

  .grid--table {
    padding: 10px 0 10px;
  }

  & > .wrapper {
    position: relative;
    z-index: 2;
    max-height: 100%;
  }

  &.is-meganav-visible {
    background: $dropdownBg !important;

    img,
    .site-nav__item > a,
    .drawer-nav__has-sublist > a,
    .site-header__cart-toggle,
    .custom-icon-hamburger,
    .drawer-nav__toggle .custom-icon-arrow-right,
    .site-header__logo a { color: $dropdownTextColor !important; }

    .site-nav__item .icon--search g,
    .site-nav__item .icon--user g { stroke: navColor($dropdownTextColor) !important; }
    .site-nav__item .icon--cart path { fill: navColor($dropdownTextColor) !important; }
  }

  &.is-moved-by-drawer { transition: $drawerTransition, background .2s ease; }

  h1 {line-height:100%}
}

.mobile-toggle-menu > * {
  @include at-query($max, $preLargest) {
    padding: 6px 10px;
    vertical-align: middle;

    &.currency-picker { padding: 0; }
  }

  @include at-query($max, $medium) {
    padding: 10px;

    &.site-header__cart-toggle {
      padding-right: 0;
    }
  }
}

#shopify-section-header {
  position: relative;
  width: 100%;
  left: 0;
  right: 0;
  z-index: 999;

  &.fixed {
    position: absolute;

    .has-scrolled & {
      position: fixed;
      top: 0;
    }
  }

  .has-slideshow & {
    &:not(.fixed) {
      position: absolute;
    }
  }
}

.site-header__logo {
  position: relative;
  z-index: 5;
  text-align: left;
  margin: 0 auto;
  max-width: 100%;
  display: flex;
  align-items: center;
  line-height:100%;
  color: $colorHeaderText;

  a {
    color: inherit;
    @include size-header(18px);
  }

  @include at-query ($min, $large) {
    text-align: left;
    margin-top: 5px;
    margin-bottom: 5px;
  }

  a,
  a:hover,
  a:focus {
    text-decoration: none;

    animation: none;
    -webkit-mask-image: none;
    -webkit-mask-size: auto;
  }

  img {
    display: block;
    height: auto;
    margin: 0 auto;
    object-fit: contain;
    object-position: left;
  }

  @include at-query ( $max, $largish ) {
    position: relative;
  }
}

.site-header__logo-link {
  margin: 0 auto;
  padding-right: 10px;
  position: relative;
  display: block;
  line-height: 1;
  font-family: $font-stack-heading;
  font-weight: $font-weight-heading;
  font-style: $font-style-heading;
  @include at-query ( $max, $small ) {
    margin: 0;
  }
}
.site-header__logo-link {
  display: none;
}
.has-no-slideshow .site-header__logo-link--other { display: block; }

// Transparent header
.supports-js {
  .has-slideshow {
    .site-header {
      &:not(.has-scrolled) {
        background: $colorHeaderBgHome;

        img,
        .site-nav__item > a,
        .drawer-nav__has-sublist > a,
        .site-header__cart-toggle,
        .custom-icon-hamburger,
        .drawer-nav__toggle .custom-icon-arrow-right,
        .site-header__logo a {
          color: navColor($colorHeaderTextHome);
        }

        .site-nav__item {
          .icon--search,
          .icon--user {
            g {
              stroke: navColor($colorHeaderTextHome);
            }
          }

          .icon--cart {
            path {
              fill: navColor($colorHeaderTextHome);
            }
          }
        }
      }

      &.fixed.color-fix {
        img,
        .site-nav__item > a,
        .drawer-nav__has-sublist > a,
        .site-header__cart-toggle,
        .custom-icon-hamburger,
        .drawer-nav__toggle .custom-icon-arrow-right,
        .site-header__logo a {
          color: opposite( $colorHeaderTextHome );
        }

        .site-nav__item {
          .icon--search,
          .icon--user {
            g {
              stroke: opposite( $colorHeaderTextHome );
            }
          }

          .icon--cart {
            path {
              fill: opposite( $colorHeaderTextHome );
            }
          }
        }
      }
    }

    .nav-search { color: navSearchHomeColor($colorHeaderTextHome); }
    .nav-search::before { background-color: $colorHeaderBgHome; }
    .nav-search__input {
      color: navSearchHomeColor($colorHeaderTextHome);

      @include at-query($min, $largest) {
        border-color: navSearchHomeColor($colorHeaderTextHome);
      }

      &::-webkit-input-placeholder { color: navSearchHomeColor($colorHeaderTextHome); }
      &::-moz-placeholder { color: navSearchHomeColor($colorHeaderTextHome); }
      &:-ms-input-placeholder { color: navSearchHomeColor($colorHeaderTextHome); }
      &:-moz-placeholder { color: navSearchHomeColor($colorHeaderTextHome); }
    }
    .nav-search__submit svg,
    .nav-search__label-icon svg { stroke: navSearchHomeColor($colorHeaderTextHome); }
    .nav-search__close path { fill: navSearchHomeColor($colorHeaderTextHome); }

    .results { color: navSearchHomeColor($colorHeaderTextHome); }

    .site-header__logo-link--other { display: none; }
    .site-header__logo-link--home,
    .site-header__logo-link--single { display: block; }

    .main-content { padding: 0px; }

    .site-header.is-meganav-visible,
    &.has-scrolled .site-header.fixed, {
      .site-header__logo-link--home { display: none; }
      .site-header__logo-link--other,
      .site-header__logo-link--single { display: block; }
    }
  }

  .has-scrolled,
  .template-collection.has-scrolled,
  .template-list-collections.has-scrolled {
    .site-header.fixed {
      background-color: $colorHeaderBgScroll;

      img,
      .site-nav__item > a,
      .drawer-nav__has-sublist > a,
      .site-header__cart-toggle,
      .custom-icon-hamburger,
      .drawer-nav__toggle .custom-icon-arrow-right,
      .site-header__logo a {
        color: navColor($colorHeaderTextScroll);
      }

      .site-nav__item {
        .icon--search,
        .icon--user {
          g {
            stroke: navColor($colorHeaderTextScroll);
          }
        }

        .icon--cart {
          path {
            fill: navColor($colorHeaderTextScroll);
          }
        }
      }

      .nav-search { color: $colorHeaderTextScroll; }
      .nav-search::before { background-color: $colorHeaderBgScroll; }
      .nav-search__input {
        color: $colorHeaderTextScroll;

        @include at-query($min, $largest) {
          border-color: $colorHeaderTextScroll;
        }

        &::-webkit-input-placeholder { color: $colorHeaderTextScroll; }
        &::-moz-placeholder { color: $colorHeaderTextScroll; }
        &:-ms-input-placeholder { color: $colorHeaderTextScroll; }
        &:-moz-placeholder { color: $colorHeaderTextScroll; }
      }
      .nav-search__submit svg,
      .nav-search__label-icon svg { stroke: $colorHeaderTextScroll; }
      .nav-search__close path { fill: $colorHeaderTextScroll; }

      .results { color: $colorHeaderTextScroll; }

      .main-content { padding: 0px; }
    }
  }
}

.site-header__cart-toggle {
  display: inline-block;
  position: relative;
  vertical-align: middle;
  color: $colorHeaderText;
  margin: 0;
  @include size-body(14px);
  @include at-query ( $max, $largish ) {
    &:hover,
    &:active,
    &:focus {
       color: darken($colorHeaderText, 10%);
    }
  }
}
.site-header__search svg {
  g {
    stroke: $colorHeaderText;
  }

  path {
    fill: none;
  }
}

.search-bar {
  max-width: 100%;
  @include at-query ($max, $medium) {
    margin-left: auto;
    margin-right: auto;
  }
}

/*============================================================================
  #Site Nav and Dropdowns
==============================================================================*/
.nav-bar {
  background-color: $colorTextBody;
}
.site-nav {
  @include size-body(16px);
  cursor: default;
  margin: 0 0 0 (-$gutter / 2);
  list-style: none;
  display: flex;
  align-items: center;
  justify-content: flex-end;

  @include at-query ( $min, $large ) {
    position: absolute;
    top: 0;
    right: 50px;
    height: 100%;
  }
}

.site-header[data-nav-position="center"]:not(.site-header--force-nav-right) {
  @include at-query ( $min, $large ) {
    .site-nav:not(.site-nav--compress) {
      left: 0;
      right: 0;
      width: 100%;
      text-align: center;
      display: flex;
      align-items: center;
      justify-content: center;
      margin: 0;
      padding-left: 0;

      .site-nav__item:not(.site-nav__item--icon) {
        padding-left: 10px;
        padding-right: 10px;
      }

      .site-nav__icons {
        position: absolute;
        top: 0;
        height: 100%;
        display: flex;
        align-items: center;
        justify-content: flex-end;
        right: 50px;
      }
    }
  }
}

.site-nav__item {
  position: relative;
  display: flex;
  align-items: center;
  margin: 0;
  padding-right: 20px;

  &:last-child {
    padding-right: 0;
  }

  li {
    display: block;
  }

  .custom-icon-hamburger,
  .dropdown-sub-links__toggle { color: $colorHeaderText; }

  .custom-icon-arrow-right {
    display: block;
    color: $colorHeaderText;
    @include transform( rotate(90deg) );
  }

  .icon {
    width: 22px;
    height: 22px;
  }

  .icon--search,
  .icon--user {
    g {
      stroke: $colorHeaderText;
    }
  }

  .icon--cart {
    height: 27px;
    margin-bottom: 2px;
    path {
      fill: $colorHeaderText;
    }
  }

  .site-nav__dropdown {
    .custom-icon-arrow-right,
    .dropdown-sub-links__toggle {
      color: $dropdownTextColor;
    }
  }

  @include at-query ( $max, 1200px ) {
    &:not(.site-nav__item--icon) {
      padding-right: 10px;
    }
  }
}

.dropdown__container {
  display: flex;
  padding: 20px 0;
}

.dropdown__container--reversed {
  flex-direction: row-reverse;
}

.dropdown__image {
  width: 220px;
  position: relative;

  img {
    display: block;
    width: 100%;
    max-height: 500px;
    object-fit: contain;
  }

  a {
    padding: 0;
  }
}

.dropdown__image--left {
  margin-left: 20px;
  margin-right: 5px;
  padding-right: 20px;
  border-right: 1px solid $colorBorder;
}

.dropdown__image--right {
  margin-right: 20px;
  margin-left: 5px;
  padding-left: 20px;
  border-left: 1px solid $colorBorder;
}

.dropdown__image-img {
  position: relative;

  .placeholder-svg {
    .grid__item & {
      position: relative;
    }
  }

  .placeholder-svg-filled {
    display: block;
    width: 100%;
    height: 200px;
    background-color: #fafafa;
  }
}

.dropdown__links {
  max-width: 320px;
  margin-top: -12px;

  .site-nav__link {
    display: block;
  }

  .has-sub-links {
    a {
      padding-right: 50px;
    }
  }

  .sub-dropdown-links {
    display: none;
    padding-left: 20px;
    margin-bottom: 5px;
  }

  .dropdown-sub-links__toggle {
    position: absolute;
    top: 0;
    right: 0;
    height: 100%;
    line-height: inherit;
    padding: 8px 10px;
    display: flex;
    align-items: center;
    justify-content: center;

    .icon {
      color: inherit;
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .icon-minus {
      display: none;
    }

    &.is-expanded {
      .icon-minus {
        display: flex;
      }
      .icon-plus {
        display: none;
      }
    }
  }
}

.site-nav__dropdown-links {
  margin-right: 15px;

  & > li:first-child { padding-top: 0; }
  .dropdown__container--reversed & {
    margin-right: 0;
  }
}

.site-nav__item--icon {
  a {
    height: 24px;
    display: flex;
    align-items: center;
  }

  @include at-query ( $max, $largish ) {
    display: none;
  }
}

.site-nav__icons {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  margin: 0;
  padding: 0;

  & > ul {
    display: flex;
    align-items: center;
    margin: -2px 0 0;
  }
}

#CartCount {
    bottom: -8px;
    position: absolute;
    left: 10px;
    max-width: 55px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: block;
    text-align: center;
    font-style: normal;
    color: opposite($colorAccent);
    font-weight: $font-weight-bold;
    text-transform: none;
    text-transform: none;
    background: $colorAccent;
    border-radius: 50%;
    min-width: 18px;
    height: 18px;
    padding: 0 3px;
    font-size: 11px;
    line-height: 18px;
    display: inline-block;
    vertical-align: top;

  @include at-query ( $max, $largish ) {
    display: none;
  }
}

.inline-parenthesis {
  display: inline-block;
}

.drawer-nav__toggle {
  padding: 10px;
  vertical-align: middle;
  pointer-events: none;
}

.drawer-nav__toggle {
  .custom-icon-arrow-right:before{
    vertical-align: bottom;
  }
}

.sub-drawer-nav__toggle {
  @extend .drawer-nav__toggle;
  position: absolute;
  top: 3px;
  right: 10px;
  .custom-icon-arrow-right { color: $dropdownTextColor !important; }
  @include transform( rotate(-90deg) );
  @include transition( all, 0.2s, ease );
}

/*================ Home to hamburger and cart that toggle drawers ================*/
.site-nav--mobile {
  @extend .site-nav;

  .text-right & {
    margin: 0 (-$gutter / 2) 0 0;
  }
}

.site-nav__link {
  @include size-body(14px);
  display: inline-block;
  vertical-align: middle;
  text-decoration: none;
  padding:15px 25px;
  padding-left: 0px;
  white-space: nowrap;
  color: $colorHeaderText;

  .site-nav--mobile & {
    display: inline-block;
  }

  &:active {
    text-decoration: underline;
  }

  &:hover,
  &:active,
  &:focus {
    color: darken($colorHeaderText, 10%);
  }

  @include at-query ( $max, $largish ) {
    padding: 15px 20px;
  }

  @include at-query ( $max, $small ) {
    padding: 0;
  }
}

.custom-icon-hamburger {
  font-size: 22px;
  float:right;
  color: $colorHeaderText;
}


.header-offset{display:none;}


/*================ Dropdowns ================*/
.site-nav__dropdown {
  display: none;
  background: $dropdownBg;
  position: absolute;
  top: 100%;
  left: 0;
  margin: 0;
  border-radius: 3px;
  border: 1px solid $dropdownLight;
  text-align: left;

  .supports-no-touch .site-nav--has-dropdown:hover &,
  .site-nav--has-dropdown.nav-hover &,
  .nav-focus + & {
    display: block;
  }

  a {
    color: $dropdownTextColor;
    padding: 8px 15px;

    &:hover,
    &:active,
    &:focus {
       color: darken($dropdownTextColor, 10%);
    }
  }

  .dropdown__image a { padding: 0; }

  li {
    position: relative;
    border-bottom: 1px solid lighten($dropdownTextColor, 80%);

    &:last-of-type {
      border-bottom: none;
    }
  }
}

.site-nav__dropdown--right {
  left: auto;
  right: 20px;
}

.cssmenu ul ul li:last-child {
  border-bottom: none;
}


.site-nav--compress {
  .site-nav__expanded-item {
    display: none;
  }
}

.site-nav--compress__menu {
  display: none;

  .site-nav--compress & {
    display: inline-block;
  }
}

.drawer-nav__has-sublist {
  display:inline-block;
  margin-right: -20px;
}

button:focus {outline:0;}



.megamenu__links {position: relative;}

.site-nav__links--default {
  position: relative;

  .has-sub-links:hover {
    & + .sub-sub-links{
      opacity: 1;
      visibility: visible;
    }
  }
  .sub-sub-links:hover{
    opacity: 1;
    visibility: visible;
  }

  .has-sub-links {
    margin-bottom:0px;
    a { padding-right: 50px; }

    &:hover > .site-nav__link + .sub-drawer-nav__toggle { right: 6px; }
  }

  .sub-sub-links {
    border-radius: 3px;
    opacity: 0;
    visibility: hidden;
    min-width: 100%;
    position: absolute;
    top: -5px;
    left: 100%;
    background: $dropdownBg;
    border: 1px solid $dropdownLight;
    border-top-left-radius: 0px;

    &:first-of-type {
      top:-1px;
    }

    .site-nav__link {
      display: block;
      width: 100%;
      border-left: 0;
      padding-right: 25px;
      padding-left: 25px;
    }

    .site-nav__link:first-of-type {
      margin: 0 0 0 -1px;
      padding-left: 26px;
      background: $dropdownBg;
      position: relative;
    }
  }

  &.site-nav__links:first-of-type {
    .sub-sub-links {
      .site-nav__link:first-of-type {
        margin: -1px 0 0 -3px;
        padding-left: 28px;
        border-top: 1px solid $dropdownLight;
      }
    }
  }

  &.site-nav__links:last-of-type {
    .sub-sub-links {
      .site-nav__link:first-of-type {
        margin: 0 0 -1px -3px;
        padding-left: 28px;
      }
    }
  }
}

/* effect-shine */
@-webkit-keyframes shine {
  from {
    -webkit-mask-position: 150%;
  }

  to {
    -webkit-mask-position: -50%;
  }
}

/*============================================================================
  #Mega menu
==============================================================================*/
.site-nav__item--has-meganav {
  position: static;
  padding-bottom: 10px;
  margin-bottom: -10px;

  &:hover {
    padding-bottom: 30px;
    margin-bottom: -30px;
    height: 100%;
    display: flex;
    align-items: center;
  }
}
.megamenu {
  position: absolute;
  top: 100%;
  right: -50px;
  width: 100vw;
  z-index: -1;
  text-align: left;
  color: $dropdownTextColor;
  background: $dropdownBg;
  box-shadow: 0 1px 3px rgba(0, 0, 0, .15);
  transition: opacity .2s ease, visibility 0s linear .2s;
  max-height: calc(75vh - 109px);
  opacity: 0;
  visibility: hidden;
  overflow-x: hidden;
  overflow-y: auto;
  pointer-events: none;

  [data-nav-position="center"]:not(.site-header--force-nav-right) & {
    right: 0;
  }

  .nav-hover & {
    opacity: 1;
    visibility: visible;
    transition: opacity .2s ease, visibility 0s linear 0s;
    pointer-events: auto;
  }

  .megamenu__links {
    float: left;
    width: calc(25% - 50px);
    margin: 0 25px;
    padding: 0 50px 50px;

    &::before {
      content: '';
      position: absolute;
      top: 0;
      left: -25px;
      width: 2px;
      height: 100%;
      background: $colorBorder;
      opacity: .25;
    }

    &:nth-child(4n+1) {
      clear: both;

      &::before {
        content: none;
      }
    }

    .site-nav__link {
      display: block;
      padding: 5px 0;
      @include size-body(14px);
      color: inherit;

      span {
        display: inline-block;
      }

      &:hover {
        opacity: .85;
      }
    }

    & > li > .site-nav__link {
      display: block;
      margin-top: 20px;
      margin-bottom: 16px;
      padding-bottom: 8px;
      opacity: 1;
      border-bottom: 1px solid $colorBorder;
      @include size-body(18px);
    }

    .sub-drawer-nav__toggle {
      display: none;
    }
  }

  .sub-sub-links {
    display: block;
    position: static;
    border-radius: 0;
    background: transparent;
    border: 0;

    .site-nav__link {
      padding: 0;
      border: 0;
      margin-bottom: 6px;
    }
  }
}

.megamenu__push + .megamenu__links::before,
.megamenu__images + .megamenu__links::before {
  content: none;
}

.megamenu-inner {
  padding: 30px 0;
  margin: 0 -25px;

  &::after {
    content: '';
    display: table;
    clear: both;
    line-height: 0;
  }
}

.megamenu--no-images {
  &.megamenu--links-1,
  &.megamenu--links-2,
  &.megamenu--links-3 {
    .megamenu-inner {
      display: flex;
      justify-content: center;
    }
  }
}

.megamenu__images {
  display: flex;
  width: 25%;

  .megamenu--left & {
    float: left;
  }

  .megamenu--right & {
    float: right;
  }

  .megamenu--stacked & {
    float: left;
    flex-direction: column;
    width: 25%;

    .megamenu__image {
      width: calc(100% - 50px);
    }
  }

  .site-nav--no-links & {
    margin: 0 auto;
    float: none;
  }
}
.megamenu__images--2 {
  width: 50%;

  .megamenu__image {
    width: calc(50% - 50px);
  }
}
.megamenu__images--3 {
  width: 75%;

  .megamenu__image {
    width: calc(33.33% - 50px);
  }
}
.megamenu__images--4 {
  width: 100%;

  .megamenu__image {
    width: calc(25% - 50px);
  }
}

.megamenu__image {
  width: calc(100% - 50px);
  margin: 0 25px 25px;
  text-align: center;
  position: relative;

  img {
    display: block;
    width: 100%;
  }
}

.megamenu__image-title {
  @extend .btn;
  position: relative;
  display: block;
  color: $colorBtnText !important;
  padding: 5px 0;
  @include size-body(12px);

  &:hover,
  &:active,
  &:focus {
    background: darken($colorBtnPrimary,10%);
    border: 2px solid darken($colorBtnBorder,10%);
    color: darken($colorBtnText,10%);
  }
}

/*============================================================================
  #Mobile Nav
  - List of items inside the mobile drawer
==============================================================================*/
.mobile-nav {
  // Negative of .drawer left/right padding for full-width link tap area
  margin: -9px 0 0;
}

.mobile-nav__search {
  padding: $gutter / 2;

  .search-bar {
    margin-bottom: 0;
  }
}

.mobile-nav__item {
  position: relative;
  display: block;
  padding-bottom: 5px;
  margin-bottom: 7px;

  // Background color on top level items so there is no
  // element overlap on subnav's CSS toggle animation
  .mobile-nav > & { background-color: $colorBody; }
  .mobile-nav > &:last-child:after { display: none; }
}
.mobile-nav__item--currency-picker { margin-top: 15px; }

// Login/logout links can't have a class on them, so style <a> element
.mobile-nav__item a {
  display: block;
}

.mobile-nav__item a,
.mobile-nav__toggle button {
  color: $colorTextHeading;
  text-decoration: none;

  &:active,
  &:focus {
   background-color: setActiveDrawers($colorBody);
  }

  .icon.icon-minus {
    display: none;
  }

  &.is-expanded {
    & > .icon {
      &.icon-plus {
        display: none;
      }

      &.icon-minus {
        display: block;
      }
    }
  }
}

.mobile-nav__item--active {
  font-weight: inherit;
}

.mobile-nav__has-sublist {
  width: 100%;
  position: relative;

  .mobile-nav__link {
    display: block;
    h5 {margin-bottom:0px;}
  }
}

.mobile-nav__link h3 {
  line-height:1;
  margin: 2px 0;
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
}

.mobile-nav__link {
  padding: 5px 0;
}

.mobile-nav__toggle {
  position: absolute;
  top: -2px;
  right: -10px;
  margin-bottom: 0px;
  z-index: auto;
  color: $colorTextHeading;

  button {
    width: 36px;
    height: 36px;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
  }

  .icon { font-size: 16px; }
}

.mobile-nav__toggle-open {
  .mobile-nav--expanded & {
    display: none;

    &.is-expanded {
      display: block;
    }
  }
}

.mobile-nav__toggle-close {
  display: none;

  .mobile-nav--expanded & {
    display: block;
  }
}

.mobile-nav__sublist {
  margin: -3px 0 0;
  display: none;
  overflow: hidden;
  margin-left: 45px;
  padding: 10px 0;
  @include backface();

  &:last-child { padding-bottom: 0px; }

  h5 {
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
  }

  .mobile-nav--expanded + & {
    visibility: visible;
    max-height: 700px;
    transition: all 700ms cubic-bezier(0.57, 0.06, 0.05, 0.95);
  }

  .mobile-nav__item {
    border: 0;
    margin-bottom: 8px;
    padding-bottom: 0;

    &:last-child { margin-bottom: 0; }

    &::after {
      top: 0;
      bottom: auto;
    }
  }
}

.mobile-nav__subsublist {
  display: none;
  border-left: 1px solid rgba($colorBorder, .5);
  padding-left: 17px;
  margin: 5px 0 5px 32px;

  h5 {
    font-size: $baseFontSize*.9;
  }

  .mobile-nav__link {
    padding: 5px 0;
  }
}

.show-dropdown {
  visibility: visible;
  max-height: inherit;
}

.js-drawer-close {
  .custom-icon-x{
    color: $colorTextHeading;
  }
}


/*============================================================================
  #Drawers
==============================================================================*/

.drawer {
  @include promote-layer();
  display: none;
  position: fixed;
  overflow-y: auto;
  overflow-x: hidden;
  top: 0;
  bottom: 0;
  padding: 0 ($gutter / 2) ($gutter / 2);
  max-width: 95%;
  z-index: $zindexDrawer;
  color: $colorDrawerText;
  background-color: $colorBody;
  transition: $drawerTransition;

  a {
    color: $colorTextHeading;
    &:hover,
    &:focus,
    &:active {
      transition: all 0.2s ease;
      color: darken($colorTextHeading,20%);
    }
  }

  input {
    border: none;
  }
  textarea {
    border-color: $colorBorder;
  }
}

.drawer--left {
  width: $drawerNavWidth;
  right: -$drawerNavWidth;
  border-right: 1px solid $colorDrawerBorder;
  border-left: 2px solid $colorAccent;

  .js-drawer-open-left & {
    display: block;
    @include transform(translateX(-$drawerNavWidth));

    .lt-ie9 & {
      left: 0;
    }
  }
}

.drawer--right {
  width: $drawerCartWidth;
  right: -$drawerCartWidth;

  border-left: 2px solid $colorAccent;

  .js-drawer-open-right & {
    display: block;
    @include transform(translateX(-$drawerCartWidth));

    .lt-ie9 & {
      right: 0;
    }
  }
}

#PageContainer {
  background: inherit;

  .full-header & {
    margin-top: -90px;
  }
}

.is-moved-by-drawer {
  @include promote-layer();

  will-change: initial;
  transition: $drawerTransition;
  .js-drawer-open-left & { @include transform(translateX(-$drawerNavWidth)); }
  .js-drawer-open-right & { @include transform(translateX(-$drawerCartWidth)); }
}

.drawer__header {
  display: table;
  height: 70px;
  width: 100%;
}

.drawer__title,
.drawer__close {
  display: table-cell;
  vertical-align: middle;
}

.drawer__title {
  width: 100%;
}

.drawer__close {
  width: 1%;
  text-align: center;
  font-size: em(18px);
}

.drawer__close button {
  position: relative;
  right: -20px;
  height: 100%;
  padding: 0 20px;
  color: inherit;

  &:active,
  &:focus {
    .custom-icon-x {
      color: lighten($colorTextHeading, 20%) !important;
    }
  }
  .custom-icon-x{
    font-size:20px;
    vertical-align: bottom;
  }
}

.cart-drawer-spacer-line{
  border-bottom: 2px solid $colorAccent;
  clear: both;
  display: block;
  width: 100%;
  margin-bottom: $gutter/2;
}
.drawer-button{margin-top:20px;}

#NavDrawer button {
  float: right;
  z-index: 1000;
}

/*============================================================================
  #Nav search
==============================================================================*/
.nav-search {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 100vh;
  z-index: 2;
  padding-top: 58px;
  box-sizing: content-box;
  opacity: 0;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
  visibility: hidden;
  transition: $drawerTransition, opacity .35s ease, visibility 0s linear .35s;
  will-change: initial;
  color: $dropdownTextColor;
  background-color: #fff; /* Fallback background color in case header background is set to transparent */

  .search-form {
    position: relative;
    width: 100%;
    max-width: 100%;
    margin: calc(50vh - 85px) auto 50px;
    @include transition(unquote('margin-top .25s ease-in'));
  }

  @include at-query($max, $preLargest) {
    display: none !important;
    visibility: hidden;
  }

  &::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: $colorHeaderBg;
    z-index: -1;
  }

  a {
    color: inherit;
  }
}
.nav-search--is-visible {
  opacity: 1;
  visibility: visible;
  transition: $drawerTransition, opacity .2s ease-out, visibility 0s linear 0s;
}
.nav-search__container {
  max-width: 85%;
  width: 1186px;
  margin: 0 auto;

  &.is-searching {
    .results::before {
      content: '';
      display: block;
      width: 32px;
      height: 32px;
      background: url({{ 'loading.svg' | asset_url }}) no-repeat center center;
      background-size: contain;
      margin: 39px auto 39px;
    }

    .results__popular-links + .results::before {
      margin-top: -27px;
      margin-bottom: 31px;
    }
  }

  &.push-up {
    .search-form {
      margin-top: 20px;
    }
  }
}
.nav-search__scroller {
  max-height: calc(100vh - 60px);
  overflow-x: hidden;
  overflow-y: scroll;
}
.nav-search__input-group {
  position: relative;
  height: 40px;
  margin: 120px auto 0;
  width: 90%;

  @include at-query($min, $large) {
    margin: 0 auto;
    width: 100%;
    height: 50px;
  }
}
.nav-search__input {
  height: 100%;
  line-height: normal;
  padding: 0 30px 0 0;
  width: 100%;
  border: 0;
  border-radius: 0;
  text-align: left;
  -webkit-appearance: none;
  box-sizing: border-box;
  color: navSearchColor($colorHeaderText);

  @include at-query($min, $largest) {
    padding-right: 40px;
    height: 100%;
    box-sizing: border-box;
    box-shadow: none;
    border-bottom: 2px solid navSearchColor($colorHeaderText);
    outline: none;
    font-size: 24px;
    font-weight: 300;
  }

  &::-webkit-input-placeholder { color: navSearchColor($colorHeaderText); }
  &::-moz-placeholder { color: navSearchColor($colorHeaderText); }
  &:-ms-input-placeholder { color: navSearchColor($colorHeaderText); }
  &:-moz-placeholder { color: navSearchColor($colorHeaderText); }
}
.nav-search__close-container {
  position: absolute;
  top: 18px;
  left: 0;
  width: 100%;
  z-index: 2;

  .inner-wrapper {
    width: 1186px;
    max-width: 85%;
    padding: 0;
    margin: 0 auto;
  }
}
.nav-search__close {
  position: relative;
  float: right;
  margin-right: -13px;
  width: 44px;
  height: 44px;
  margin-top: -10px;
  display: flex;
  align-items: center;
  background: $colorHeaderBg;
  cursor: pointer;

  path {
    fill: navSearchColor($colorHeaderText);
  }
}
.nav-search__submit {
  display: block;
  position: absolute;
  top: 0;
  right: 0;
  height: 100%;
  display: flex;
  align-items: center;

  svg {
    width: 25px;
    height: 25px;
    stroke: navSearchColor($colorHeaderText);

    g {
      stroke: inherit;
    }
  }
}

/*============================================================================
  #Nav search collection links
==============================================================================*/
.collection-links {
  margin: 20px 0;
}
.collection-links__heading {
  margin-bottom: 20px;
  opacity: .7;
}
.collection-links__list {
  list-style: none;
  margin: 0;
  display: flex;
  flex-wrap: wrap;
}
.collection-links__list-item {
  margin: 0 6px 0 0;
  font-size: $baseFontSize * 2.2;
  line-height: 1.2;
  font-weight: $font-weight-bold;

  &::after {
    content: '/';
    margin-left: 6px;
  }

  &:last-child::after {
    content: none;
  }

  a {
    color: inherit;
    &:hover {
      opacity: .7;
    }
  }
}

/*============================================================================
  #Nav search results
==============================================================================*/
.results {
  color: $colorHeaderText;

  .pagination {
    margin: 20px 0 40px;

    .loadMore {
      margin: 0;
    }
  }

  .no-results {
    margin: 44px 0;
    text-align: center;
  }

  .loadMore--endlessScroll {
    visibility: hidden;
    opacity: 0;
  }

  a {
    color: inherit;
  }

  h3 {
    color: inherit;

    a {
      color: inherit !important;
    }
  }
}

.results__popular-title {
  @include size-header(14px);
  text-align: left;
  margin: 0 0 13px;
}

.results__popular-links {
  @include size-header(18px);
  text-align: left;
  margin: 0 0 45px;

  li {
    display: inline-block;
    margin: 0 20px 20px;

    &:first-child {
      margin-left: 0;
    }

    &:last-child {
      margin-right: 0;
    }
  }

  & + .results {
    .results__count,
    .no-results {
      margin-top: -30px;
    }
  }
}

.results__count {
  text-align: center;
  margin: 0 0 30px;
}

.results__list {
  list-style: none;
  margin: 20px -10px 0;
  display: flex;
  flex-wrap: wrap;

  .result {
    width: calc(25% - 20px);
    margin: 0 10px 40px;
    text-align: center;
  }

  .result__image {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 200px;
    margin-bottom: 15px;

    a {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      height: 100%;
      position: relative;
      cursor: default;
    }

    img {
      display: block;
      width: 100%;
      height: 100%;
      max-width: 100%;
      max-height: 100%;
      object-fit: contain;
      height: auto;
      cursor: pointer;
    }

    .product-status-flag {
      display: flex;
      align-items: center;
      justify-content: center;
    }
  }

  .result__title {
    margin: 0;
    font-size: $baseFontSize;
  }
}

/*============================================================================
  #Product and Collection Grids
==============================================================================*/
.grid__image {
  position: relative;
  display: block;
  overflow: hidden;
  margin: 0 auto ($gutter / 2);
  img {
    display: block;
    margin: 0 auto;
  }

  &:hover {
    cursor: pointer;
  }
  .lazyload {display: none;}
}

.supports-js .grid__image .lazyload {display: block;}

/*============================================================================
  #Collection Filters
==============================================================================*/
.filter--active {
  font-weight: $font-weight-bold;
}

/*============================================================================
  #Breadcrumbs
==============================================================================*/
.breadcrumbs {
  margin: $gutter/2 0;

  a,
  span {
    display: inline-block;
    padding: 0 7px 0 0;

    &:first-child {
      padding-left: 0;
    }
  }
}

/*============================================================================
  #Notes and Form Feedback
==============================================================================*/
.note,
.errors {
  border-radius: $radius;
  padding: 6px 12px;
  margin-bottom: $gutter / 2;
  border: 1px solid transparent;
  @include size-body(12px);
  text-align: center;

  ul,
  ol {
    margin-top: 0;
    margin-bottom: 0;
    list-style: none outside;
  }

  li:last-child {
    margin-bottom: 0;
  }

  p {
    margin-bottom: 0;
  }
}

.errors {
  background-color:white;
  ul {
    list-style: none outside;
    margin-left: 20px;
  }

  & ~ .newsletter--form {
    #mail {
      color: $errorRed;
      border-color: $errorRed;
    }
  }
}

.form-success {
  color: $successGreen;
  border-color: $successGreen;

  a {
    color: $successGreen;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}

.form-error,
.errors {
  color: $errorRed;
  border-color: $errorRed;

  a {
    color: $errorRed;
    text-decoration: underline;

    &:hover {
      text-decoration: none;
    }
  }
}


/*============================================================================
  #Ajax Cart Styles (conditionally loaded)
==============================================================================*/
.ajaxcart__inner {
  margin-bottom: $gutter;
}

.ajaxcart__row {
  > .grid {
    margin-left: -$gutter / 2;

    > .grid__item {
      padding-left: $gutter / 2;
      .product-image {margin-top: 20px;}
    }
  }
}

.ajaxcart__product {
  position: relative;
  max-height: 500px;

  &.is-removed {
    max-height: 0;
    overflow: hidden;
    visibility: hidden;
    transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
    @include backface();
  }
}

.ajaxcart__row {
  padding-bottom: $gutter / 2;
  margin-bottom: $gutter / 2;
  p {
    margin-bottom: 0px;
  }
}

.ajaxcart__product-image {
  display: block;
  overflow: hidden;
  margin-bottom: 15px;
  margin-top:20px;

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }
}

.ajaxcart__product-name {
  display: block;
  color: $colorTextHeading !important;
  @include size-header(16px);
  &:hover,
  &:focus,
  &:active {
  color: $colorLinkHover !important;
  }
}


.ajaxcart__product-meta {
  color: $colorTextLight;
}

.ajaxcart_product-price {display:block;}

#CartContainer {
  color: $colorTextBody;
}

#CartDrawer .custom-icon-x {color: $colorTextHeading !important;}

/*============================================================================
  #Quantity Selectors
==============================================================================*/
.js-qty {
  position: relative;
  margin-bottom: 1em;
  max-width: 100px;
  min-width: 75px;
  overflow: visible;

  input {
    display: block;
    background: none;
    text-align: center;
    width: 100%;
    padding: 5px 25px;
    margin: 0;
    color: $colorTextBody;
  }
}

.js-qty__adjust {
  cursor: pointer;
  position: absolute;
  display: block;
  top: 0;
  bottom: 0;
  border: 0 none;
  padding: 0 8px;
  background: none;
  text-align: center;
  overflow: hidden;
  color: $colorTextBody;
  @include user-select(none);

}

.js-qty__adjust--plus {
  right: 0;
}

.js-qty__adjust--minus {
  left: 0;
}

/*============================================================================
  #Quantity Selectors in Ajax Cart
==============================================================================*/
.ajaxcart__qty {
  @extend .js-qty;
  margin: 0;

  .is-loading & {
    opacity: 0.5;
    transition: none;
  }
}

.ajaxcart__qty-num {
  color: colorTextBody !important;
}

.ajaxcart__qty-adjust {
  @extend .js-qty__adjust;
}

.ajaxcart__qty--plus {
  @extend .js-qty__adjust--plus;
}

.ajaxcart__qty--minus {
  @extend .js-qty__adjust--minus;
}

#CartContainer {
 .price {margin-top: auto;}
}

#CartDrawer {
  z-index: 900;
}

/*================ Slider ================*/
.secondary-call-to-action {
   @include size-body(14px);
  letter-spacing: 2px;
  position: relative;
  text-transform: uppercase;
  color: $colorTextHeading;
  font-family:$font-stack-button;
  font-weight:$font-weight-button;
  font-style:$font-style-button;

  &:hover,
  &:focus,
  &:active {
      transition: all 0.2s ease;
      color: darken($colorTextHeading,10%);
  }

  span {
    display: inline-block;
    vertical-align: middle;
  }
}
.btn.call-to-action.hero-btn {
  border: 2px solid;
  transition: all .3s;
  -webkit-transition: all .3s;
  -moz-transition: all .3s;
  -ms-transition: all .3s;
  transition: all .3s;
}

.filter{
  position: absolute;
  background-color:rgba(0,0,0,.4);
  width: 100%;
  height: 100%;
  top:0px;
  z-index: 1;
  display: none;
}

.call-to-action-circle{
  position: relative;
  display: inline-block;
  font-size: 30px;
  line-height: 1;
  vertical-align: middle;
}

.call-to-action-triangle{
  @include size-body(14px);
  margin: auto;
  position: absolute;
  top: 50%;
  left: 50%;
  width: 10px;
  height: 15px;
  margin-top: -11px;
  margin-left: -12px;
}

.slider {
  position: relative;
  z-index: 5;

  .flickity-page-dots {
    bottom: 30px;
    padding: 0 20px;
    margin-bottom: 15px;
  }

  &[data-current-style="text-dark"] {
    .slider__scroll-down path {
      fill: black;
    }

    .flickity-page-dots .dot { background: black; }
    .flickity-prev-next-button .arrow { fill: black; }
    .slider__arrow { color: black; }
  }

  &[data-current-style="text-light"] {
    .slider__scroll-down path {
      fill: white;
    }

    .flickity-page-dots .dot { background: white; }
    .flickity-prev-next-button .arrow { fill: white; }
    .slider__arrow { color: white; }
  }
}

.slider--zoom-out {
  img,
  .slide-background {
    transform: scale(1.15);
    transition: transform 0s ease-out .5s !important;
  }
  .is-selected img.lazyloaded,
  .is-selected .slide-background.lazyloaded,
  .is-selected .slide-background.placeholder-bg,
  .initial-slide img,
  .initial-slide .slide-background {
    transform: scale(1);
    transition: transform 3s ease-out, opacity .3s ease-in !important;
  }

  &.no-transition .slide-background,
  &.no-transition img {
    transition: none;
  }

  .flickity-slider {
    transform: none !important;
  }

  .item {
    left: 0 !important;
    opacity: 0;
    z-index: -1;
    transition: opacity 0s linear .5s;
  }

  .item.is-selected {
    opacity: 1;
    z-index: 0;
    transition: opacity .5s ease-in-out;
  }
}

.image-mobile {
  @include at-query($min, $large) {
    display: none !important;
  }

  @include at-query($max, $medium) {
    & ~ .image-desktop {
      display: none !important;

      & + .loader {
        display: none;
      }
    }
  }
}

.slider--fade {
  .flickity-slider {
    transform: none !important;
  }

  .item {
    left: 0 !important;
    opacity: 0;
    z-index: -1;
    transition: opacity 0s linear .5s;
  }

  .item.is-selected {
    opacity: 1;
    z-index: 0;
    transition: opacity .5s ease-in-out;
  }
}

.slider--dots-left {
  .flickity-page-dots {
    bottom: 10px;
    text-align: left;
  }
}

.slider--dots-center {
  .flickity-page-dots {
    text-align: center;
  }
}

.slider--dots-right {
  .flickity-page-dots {
    bottom: 10px;
    text-align: right;
  }
}

.item--center {
  .slide-inner { text-align: center; }
}
.item--left,
.item--bottom-left {
  .slider-text-block {
    text-align: left;

    .button {
      margin-left: 0;
    }
  }
}
.item--right,
.item--bottom-right {
  .slider-text-block {
    text-align: right;

    .button {
      margin-right: 0;
    }
  }
}
.item--bottom-left,
.item--bottom-right {
  .slider-text-block {
    @include at-query($min, $large) {
      top: auto;
      bottom: 0;
      transform: translate(-50%, 0);
      margin-bottom: 90px;
    }
  }
}

.item--text-light {
  .main-heading,
  .main-subheading,
  .secondary-call-to-action {
    color: white !important;
  }
}

.item--text-dark {
  .main-heading,
  .main-subheading,
  .secondary-call-to-action {
    color: black !important;
  }
}

.slider__scroll-down {
  position: absolute;
  display: inline-block;
  left: 50%;
  bottom: 0;
  width: 44px;
  height: 44px;
  margin-left: -22px;
  z-index: 10;

  .icon-arrow-right {
    display: block;
    width: 100%;
    height: 100%;
    @include transform( rotate(90deg) );
  }
}

.slide-block {
  position: relative;
  z-index: 0;
  text-align: center;
  width: auto;
}

.slide__heading {
  line-height: 1.08;
}

.slider__arrow {
  position: absolute;
  z-index: 1;
  top: 50%;
  width: 44px;
  height: 44px;
  border: none;
  border-radius: 50%;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 28px;
  line-height: 1;
  @include transform( translateY(-50%) );

  svg {
    display: block;
    max-width: 100%;
    max-height: 100%;
  }

  .no-js & {
    display: none;
  }
}

.slider__arrow--previous {
  left: 15px;
  .custom-icon-arrow-right,
  .icon-arrow-right {
    @include transform( rotate(180deg) );
  }
}
.slider__arrow--next {
  right: 15px;
}

.video-wrapper {
  position:relative;
}

.one-hundred-height-hero {height: 100vh !important;}
