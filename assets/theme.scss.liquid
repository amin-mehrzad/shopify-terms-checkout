/**
  * Rich text
  */
.rich-text {
  position: relative;
  background: #f5f5f5;
}

.rich-text--no-image {
  padding-top: 50px;
  padding-bottom: 50px;
}

.rich-text__wrapper {
  position: relative;
  margin: 75px 0;
  display: flex;
  align-items: center;
  min-height: 515px;
  padding: 40px 0;

  @include at-query($max, $largish) {
    margin: 40px 0;
    min-height: 400px;
  }

  @include at-query($max, $medium) {
    margin: 0;
    padding-bottom: 0;
    min-height: 0;
    flex-direction: column-reverse;

    .rich-text--has-background & {
      padding-bottom: 40px;
    }
  }
}

.rich-text__wrapper--reversed {
  @include at-query ($min, $large) {
    flex-direction: row-reverse;
  }
}

.rich-text__background {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}
.rich-text__background-image {
  display: block;
  width: 100%;
  height: 100%;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;
}
.rich-text__image {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  width: calc(50% + 85px);

  .rich-text__wrapper--reversed & {
    right: auto;
    left: 0;
  }

  @include at-query($max, $medium) {
    width: calc(100% + 100px);
    position: relative;
    top: auto;
    left: auto;
    right: auto;
    bottom: auto;
    height: 100vw;
    max-height: 400px;
    margin: -25% -50px 0;
  }
  @include at-query($max, $small) {
    width: calc(100% + 50px);
    margin: -50% -50px 0;
  }
}
.rich-text__image-bg {
  height: 100%;
  overflow: hidden;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: cover;

  @include at-query($max, $medium) {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
  }

  .placeholder-svg {
    max-width: none;
    margin-left: -50%;
    width: calc(100vw * 0.75);

    @include at-query($max, 1200px) {
      width: 100vw;
    }

    @include at-query($max, $largest) {
      width: 200%;
    }

    @include at-query($max, $largish) {
      width: 250%;
    }

    @include at-query($max, $medium) {
      width: 100%;
      margin-left: 0;
    }

    .rich-text__wrapper--reversed & {
      margin-left: 0;
      margin-right: -50%;

      @include at-query($max, $medium) {
        margin-right: 0;
      }
    }
  }
}
.rich-text__content {
  position: relative;
  width: calc(50% - 20px);
  margin: 0;
  background: #fff;
  color: #333;
  padding: 40px;

  @include at-query($max, $largish) {
    width: calc(60% - 20px);
  }

  @include at-query($max, $medium) {
    width: 100%;
    margin: 0 auto;
    padding: 30px;
    text-align: center;
  }
}
.rich-text__subheading {
  @include size-body(12px);
  text-transform: uppercase;
  letter-spacing: 0.15em;
  margin-bottom: 10px;
}
.rich-text__heading {
  color: inherit;
}
.rich-text__text {
  padding-bottom: 10px;
}

/**
  * Swatches Styles
  */
.swatch {
  display: flex;
  flex-wrap: wrap;
  margin: 10px 0 5px;

  .text-center & {
    justify-content: center;

    .swatch-element {
      margin-left: 5px;
      margin-right: 5px;
    }
  }

  .header {
    width: 100%;
    color: $colorPrimary;
    font-weight: $font-weight-normal;
    font-size: $baseFontSize;
    line-height: 1.5;
    text-align: left;
    margin-bottom: 3px;

    .text-center & {
      text-align: center;
    }
  }

  input {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    border-radius: 10%;
    z-index: 2;
    cursor: pointer;

    &:checked + label {
      &::after {
        opacity: 1;
        visibility: visible;
        @include transition(unquote('opacity .2s ease, visibility 0s linear 0s'));
      }
    }
  }

  label {
    position: relative;
    float: left;
    height: {{ swatch_height }} !important;
    min-width: {{ swatch_width }} !important;
    margin: 0;
    font-size: 13px;
    text-align:center;
    line-height: {{ swatch_height }};
    white-space: nowrap;
    text-transform: uppercase;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;

    span {
      position: absolute;
      width: 100%;
      height: 100%;
      border-radius: 50%;
      background-color: #ddd;
      background-position: center center;
      background-size: cover;
      background-repeat: no-repeat;
    }

    &::after {
      content: '';
      position: absolute;
      top: -4px;
      left: -4px;
      right: -4px;
      bottom: -4px;
      border: 1px solid $colorBorder;
      border-radius: 50%;
      opacity: 0;
      visibility: hidden;
      @include transition(unquote('opacity .2s ease, visibility 0s linear .2s'));
    }
  }

  .swatch-element {
    position: relative;
    z-index: auto;
    margin: 0px 10px 10px 0;
    -webkit-transform: translateZ(0); /* webkit flicker fix */
    -webkit-font-smoothing:antialiased; /* webkit text rendering fix */

    label {
      padding: 0 10px;
      cursor: pointer;
    }

    .crossed-out {
      display: none;
    }

    &.color {
      label {
        padding: 0;
      }
    }

    &.soldout {
      .crossed-out {
        display: block;
      }

      label {
        filter: alpha(opacity=60); /* internet explorer */
        -khtml-opacity: 0.6;      /* khtml, old safari */
        -moz-opacity: 0.6;       /* mozilla, netscape */
        opacity: 0.6;           /* fx, safari, opera */
      }
    }

    &.white {
      label {
        span {
          box-shadow: inset 0 0 0 1px $colorBorder;
        }
      }
    }

    @include at-query ($max, $medium) {
      float: none;
      display: inline-block;
    }

    &::after {
      content: attr( data-value );
      position: absolute;
      left: 0;
      top: 100%;
      margin-top: 8px;
      background: $colorBody;
      color: $colorPrimary;
      padding: 2px 5px;
      border-radius: 3px;
      box-shadow: 0 1px 3px rgba(0, 0, 0, .15);
      border: 1px solid $colorBorder;
      white-space: nowrap;
      opacity: 0;
      visibility: hidden;
      transition: transform .15s ease, opacity .15s ease, visibility 0s linear .15s;
      @include size-body(11px);
      @include transform(translateY(50%));
    }

    &:hover {
      z-index: 5;
      &::after {
        opacity: 1;
        visibility: visible;
        transition: transform .3s ease, opacity .3s ease, visibility 0s linear 0s;
        @include transform(translateY(0));
      }
    }
  }

  &.error {
    background-color: #E8D2D2!important;
    color: #333 !important;
    padding: 1em;

    p {
      margin: 0.7em 0;

      &:first-child {
        margin-top: 0;
      }

      &:last-child {
        margin-bottom: 0;
      }
    }

    code {
      font-family: monospace;
    }
  }
}

.crossed-out {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
}

.swatches-fake {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: -10px 0 10px;

  input,
  .header {
    display: none;
  }

  .swatch-element {
    margin: 0 4px !important;

    label {
      min-width: 12px !important;
      width: 12px !important;
      height: 12px !important;
      border: 0;
      border-radius: 0;
      cursor: default;

      span {
        width: 100%;
        height: 100%;
      }

      &::after {
        content: none;
      }
    }
  }
}

.btn + .swatches-fake { margin-top: 0; }

.payment-fake {
  display: none;
}

