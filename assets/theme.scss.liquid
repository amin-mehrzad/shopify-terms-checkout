/*============================================================================
  #product-tabs.liquid
==============================================================================*/
/**
 * Tabs
 */
.product-tabs {
  margin: 20px 0;
}
.product-tab {
  padding: 12px 0;
  border-top: 1px solid $colorBorder;
  border-bottom: 1px solid $colorBorder;
  text-align: left;

  & + .product-tab {
    margin-top: -1px;
  }
}

.product-tab-title {
  display: block;
  position: relative;
  padding: 0px 40px 0 0;
  cursor: pointer;

  .icon-toggle {
    position: absolute;
    top: 50%;
    right: 10px;
    width: 15px;
    height: 1px;

    &::before,
    &::after {
      content: '';
      position: absolute;
      left: 50%;
      top: 50%;
      width: 100%;
      height: 100%;
      background: #666;

      @include transition(transform, .5s, ease);
    }

    &::before {
      @include transform(unquote('translate(-50%, -50%) rotate(90deg)'));
    }

    &::after {
      @include transform(unquote('translate(-50%, -50%)'));
    }
  }

  &::before,
  &::after {
    content: '';
    position: absolute;
    left: 0;
    top: -12px;
    bottom: -12px;
    width: 0;
    opacity: .35;
    background: $colorBorder;
  }

  &::after {
    width: 100%;
    opacity: 0;
    -webkit-transition: width 0s linear 0s, opacity .5s ease;
    -moz-transition: width 0s linear 0s, opacity .5s ease;
    transition: width 0s linear 0s, opacity .5s ease;
  }
}

.product-tab-content {
  margin: 0;
  overflow: hidden;
  max-height: 0;
  @include transition(unquote('max-height .3s cubic-bezier(0, 1, .5, 1), margin .15s linear'));
}

.is-active {
  .product-tab-title {
    .icon-toggle {
      &::before {
        @include transform(unquote('translate(-50%, -50%) rotate(0deg)'));
      }

      &::after {
        @include transform(unquote('translate(-50%, -50%) rotate(180deg)'));
      }
    }

    &::before {
      width: 100%;
      opacity: 0;
      @include transition(unquote('width 0s linear 0s, opacity .5s ease'));
    }

    &::after {
      width: 0;
      opacity: .35;
      transition: none;
    }
  }

  .product-tab-content {
    margin: 10px 0;
    max-height: 2000px;
    @include transition(unquote('max-height .3s cubic-bezier(1, 0, 1, .5), margin .3s linear'));
  }
}


