    opacity: 0;
  }

  &.slider--disabled {
    display: flex;
    justify-content: center;
    flex-wrap: wrap;

    .slider__arrow {
      display: none;
    }
  }

  .product-grid-item__title {
    @include at-query ($max, $small) {
      display: block;
      padding-left: 15px;
      padding-right: 15px;
    }
  }

  @include at-query( $max, $postSmall ) {
    margin-left: 15px;
    margin-right: 15px;
  }
}

.featured-collection {
  text-align: center;

  .flickity-page-dots {
    left: 0;
  }

  .product-image__slider {
    & > .flickity-prev-next-button {
      &.previous {
        left: 0px;
      }

      &.next {
        right: 0px;
      }

      .arrow {
        fill: $colorTextBody;
      }
    }
  }

  .fake-collection {
    .flickity-prev-next-button {
      &.previous {
        left: -25px;
        @include at-query($min, $postSmall) {
          left: -45px;
        }
      }

      &.next {
        right: -25px;
        @include at-query($min, $postSmall) {
          right: -45px;
        }
      }
    }
  }
}

/*================ Featured Collections ================*/
.collection__tabs {
  margin-top: $gutter;
  position: relative;
}

.collection__tabs-nav {
  margin: 0 0 $gutter / 2;
  list-style: none;
  text-align: center;
  overflow: auto;
  backface-visibility: hidden;
  -webkit-backface-visibility: hidden;

  li {
    margin: 0 15px 15px;
    display: inline-block;

    &.is-active {
      button {
        opacity: 1;
      }
    }
  }

  button {
    display: block;
    cursor: pointer;
    opacity: .5;
    padding: 0 10px 5px;
    white-space: nowrap;
    border-bottom: 1px solid $colorBorder;
    font-family: $font-stack-button;
    font-weight: $font-weight-button;
    font-style: $font-style-button;
    @include size-button(16px);

    @include at-query ($max, $small) {
      @include size-button(14px);
    }

    &:hover {
      opacity: .65;
    }
  }
}

.collection__tab {
  transition: opacity .5s ease;

  &:not(.is-active) {
    position: absolute;
    visibility: hidden;
    opacity: 0;
    z-index: -1;
    transition: none;
  }
}

/* Center align product grid items if the products are less than the "products per row" setting */
.collection__tab-content:not(.featured-collection--slider),
#featured-collection .featured-collection:not(.featured-collection--slider) {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
/*================ Featured Image ================*/
.featured-image-section {
  background-repeat: no-repeat;
  -webkit-background-size: 100%;
  -moz-background-size: 100%;
  -o-background-size: 100%;
  background-size: 100%;
  background-size: cover;
  /* Preserve aspet ratio */
  min-width: 100%;
  min-height: 100%;
  text-align: center;
  display: table;
  position: relative;
}

.featured-image-section-inner {
  display: table-cell;
  vertical-align: middle;
  z-index: 10;
  position: relative;
  width: 100%;
  padding-right: 12.5%;
  padding-left: 12.5%;
}

.original-height{
  opacity: 0;
}

.no-js .original-height {opacity: 1;}

.ba-fixed {
  background-attachment: fixed;
  @include at-query ($max, $largish) {
    background-attachment: scroll;
  }
}

.bg-pos-top {background-position: top !important;}
.bg-pos-bottom {background-position: bottom !important;}
.bg-pos-center {background-position: center !important;}
.original-height--position {background-position: center top !important;}

