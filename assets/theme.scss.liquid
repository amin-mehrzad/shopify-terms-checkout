/*============================================================================
  #Invert Color Based on Parameter Color
==============================================================================*/
@function opposite($color) {
  @if (lightness($color) > 80) {
    @return #000000; // Lighter background, return dark color
  } @else {
    @return #ffffff; // Darker background, return light color
  }
}

@function whiteText($color) {
  @if (lightness($color) > 80) {
    @return $colorTextHeading; // Lighter background, return dark color
  } @else {
    @return #ffffff; // Darker background, return light color
  }
}

@function whiteTextBody($color) {
  @if (lightness($color) > 90) {
    @if (lightness($colorTextBody) < 90) {
      @return $colorTextBody;
    } @else {
      @return #000000;
    }
  } @else {
    @return #ffffff;
  }
}

@function navColor($color) {
  @if ($color == white) {
    @if ($colorHeaderBg == white) {
      @return $colorTextBody;
    } @else {
      @return $color;
    }
  } @else {
    @return $color;
  }
}

@function navSearchColor($color) {
  @if ($color == white) {
    @if ($colorHeaderBg == rgba(0, 0, 0, 0)) {
      @return $colorTextBody;
    } @else {
      @return $color;
    }
  } @else {
    @return $color;
  }
}

@function navSearchHomeColor($color) {
  @if ($color == white) {
    @if ($colorHeaderBgHome == rgba(0, 0, 0, 0)) {
      @return $colorTextBody;
    } @else {
      @return $color;
    }
  } @else {
    @return $color;
  }
}

@function whiteTextLight($color) {
  @if (lightness($color) > 80) {
     @if (lightness($colorTextLight) < 80) {
        @return $colorTextLight;
       } @else {
        @return lighten(#000000,50%);
      }
  } @else {
    @return #ffffff;
  }
}

@function whiteTextButton($color) {
  @if (lightness($color) > 80) {
    @return $colorBtnPrimary; // Lighter background, return dark color
  } @else {
    @return #ffffff; // Darker background, return light color
  }
}

@function setInstaComment($color) {
  @if (lightness($color) > 80) {
    @return $colorBody; // Lighter background, return dark color
  } @else {
   @return white;
  }
}

@function setAccountTables($color) {
  @if (lightness($color) < 80) {
    @return $colorBody;
  } @else {
   @return  $colorTextBody;
  }
}

@function setTicker($color) {
  @if (lightness($color) < 80) {
    @return white;
  } @else {
   @return $colorBorder;
  }
}

@function setTickerHover($color) {
  @if (lightness($color) < 90) {
    @return white;
  } @else {
   @return $colorBorder;
  }
}

@function setQtyInput($color) {
  @if (lightness($color) < 80) {
    @return white;
  } @else {
   @return $colorBorder;
  }
}

@function setActiveDrawers($color) {
  @if (lightness($color) > 80) {
    @return darken($colorBody,1%); // Lighter background, return dark color
  } @else {
   @return lighten($colorBody,10%);
  }
}

@function passwordButton($color) {
  @if (lightness($color) > 80) {
    @return black; // Lighter background, return dark color
  } @else {
   @return $colorBtnText;
  }
}
@function passwordButtonBg($color) {
  @if (alpha($color) < .5) {
    @return white; // Lighter background, return dark color
  } @else {
   @return $colorBtnPrimary;
  }
}

@function normalizeFC ($color){
  @if (lightness($color) > 70) {
    @return $colorBody; // Lighter background, return dark color
  } @else {
   @return $color;
  }
}

@function normalizeNewsletter($color){
  @if (lightness($colorAccent) > 90) {
    @return $colorBody; // Lighter background, return dark color
  } @else {
   @return $color;
  }
}

@function setDrawerText($color){
  @if (lightness($colorBody) > 90) {
    @return $colorTextHeading; // Lighter background, return dark color
    $colorNewDrawerText : $colorTextHeading;
  } @else {
   @return white;
    $colorNewDrawerText : white;
  }
}

/*============================================================================
  #Basic Styles
==============================================================================*/
html {
  background-color: $colorBody;
}

body {
  background-color: $colorBody;

  &.lock-body {
    overflow: hidden !important;
    position: relative;
  }

  .no-js & {
    display: block !important;
  }
}

[tabindex='-1']:focus {
  outline: none;
}

.wrapper {
  @include clearfix();
  max-width: $siteWidth;
  margin: 0 auto;
  padding: 0 ($gutter / 2);

  @include at-query ($min, $small) {
    padding: 0 $gutter;
  }
}

.main-content {
  display: block;
  padding-top: 50px;
  padding-bottom: 100px;

  .no-js & {
    padding-top: 0;
  }
}

.template-collection,
.template-list-collections {
  .main-content {
    padding-top:0px;
    padding-bottom: 50px;
  }
}

.section {
  padding-top: 50px;
  padding-bottom: 50px;
}

.body--show-spacer-lines {
  .vertical-spacer{
    border-right: 2px solid $colorAccent;
    display: block;
    height: 36px;
    margin: 0 auto;
    text-align: center;
    width: 0;
    z-index: 500;
  }
}

.body--hide-spacer-lines {
  hr {
    opacity:0;
  }

  .product-feature__text {
    hr {
      display: none;
    }
  }
}

/*============================================================================
  #Helper Classes
==============================================================================*/
.is-transitioning {
  display: block !important;
  visibility: visible !important;
}

.display-table {
  display: table;
  table-layout: fixed;
  width: 100%;
}

.display-table-cell {
  display: table-cell;
  vertical-align: middle;
  float: none;
}

@include at-query ($min, $large) {
  .large--display-table {
    display: table;
    table-layout: fixed;
    width: 100%;
  }

  .large--display-table-cell {
    display: table-cell;
    vertical-align: middle;
    float: none;
  }
}

.visually-hidden {
  position: absolute;
  overflow: hidden;
  clip: rect(0 0 0 0);
  height: 1px; width: 1px;
  margin: -1px; padding: 0; border: 0;
}

.hide{display:none;}

.template-product,
.featured-product {
    .product-page-frame .featured-image {
    height: 450px;
    @media (max-width: 767px) {
      height: 320px;
    }
  }
}
.template-product {
  .selector-wrapper label {margin-right: 10px;}

  .alt-image {
    //Adds up to 30
    margin-bottom: 23px;
  }

  .product-images {
    @include at-query ( $max, $largish ) {
      width: 100%;
      max-height: 100%;
    }
  }

  .product-description-wrapper {
    @include at-query ( $max, $largish ) {
      width: 100%;
    }
  }

  .product-page-table{
    .cell img {
      width: 75%;
      height: auto;
      display: block;
      padding: 0;
      margin: 0 auto;
    }
  }
}

.mfp-gallery {
  .mfp-container {background: white;}
  .mfp-bg {
    background-color: $colorBody !important;
    opacity: 1 !important;
  }

  .mfp-close {
    margin: 30px;
    background-image: none;
    text-indent: 0;
  }

  .mfp-zoom-in-cur {
    @include at-query ($min, $large) {
      cursor: -webkit-zoom-in;
      cursor: -moz-zoom-in;
      cursor: zoom-in;
     }
  }
}

#ProductThumbs {
  @include at-query ( $max, $largish ) {
    padding-bottom: 25px;
  }
}

