  .product-image--hover-img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
  }

  .btn-wrapper {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: auto;
    pointer-events: none;
    overflow: hidden;
    @include transform( translateY(100%) );
    @include transition( transform, .3s, ease );

    @include at-query( $max, $small ) {
      display: none;
    }

    .btn {
      display: block;
      padding: 10px;
      width: 100%;
      text-align: center;
      cursor: pointer;
      pointer-events: auto;
      position: relative;
      opacity: 0;
      @include transition( all, .3s, ease );
    }
  }

  .error {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    padding: 10px;
    border: 2px solid transparent;
    text-align: center;
    color: $colorTextBody;
    background: $colorBody;
    opacity: 0;
    @include transition( opacity, .3s, ease );
  }

  .flickity-prev-next-button {
    &:disabled {
      opacity: .25;
    }

    &.previous {
      left: 0;
    }

    &.next {
      right: 0;
    }

    .arrow {
      fill: $colorPrimary;
    }
  }

  .product-images__slider {
    .flickity-prev-next-button {
      &.previous {
        left: 10px;
      }

       &.next {
        right: 10px;
      }
    }

     svg {
      left: 0;
      top: 0;
      width: 100%;
      height: 100%;
    }
  }

  .table {
    position: relative;
    & > div {
      height: 100%;
    }
  }

  @include at-query ($min, $large) {
    .grid__image:hover {
      .product-image--hover,
      .product-image__slider {
        opacity: 1;
        visibility: visible;
      }
    }
  }

  .product-image__slider--arrows-white {
    .flickity-prev-next-button {
      .arrow {
        fill: #fff;
      }
    }
  }

  .placeholder-svg,
  .placeholder-svg-filled {
    position: absolute;
    top: 0;
    left: 0;
  }

  .no-js & {
    .cell > noscript {
      display: block;
      width: 100%;
      height: 100%;
    }
  }
}

.btn--add-to-cart {
  svg {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 32px;
    height: 32px;
    margin: -16px 0 0 -16px;
    stroke: $colorBtnText;
    opacity: 0;
    visibility: hidden;
    transition: opacity .2s ease, visibility 0s linear .2s;
  }

   span {
    transition: opacity .2s ease, visibility 0s linear 0s;
  }

   &.is-loading {
    svg {
      opacity: 1;
      visibility: visible;
      transition: opacity .2s ease, visibility 0s linear 0s;
    }

     span {
      opacity: 0;
      visibility: hidden;
      transition: opacity .2s ease, visibility 0s linear .2s;
    }
  }
}

/* Collection sorting */
#collection-sorting {
  .form-horizontal {
    float: right;
    padding: 20px 0;
  }

  label {
    display: block;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    margin-bottom: 5px;
    @include size-body(10px);
  }

  &::after {
    content: '';
    display: table;
    clear: both;
    line-height: 0;
  }

  @include at-query ( $max, $medium ) {
    text-align: center;

    .form-horizontal {
      float: none;
      display: inline-block;
    }
  }
}

/* Sub-collections */
.subcollections {
  padding-bottom: 20px;
}
.pagination + .subcollections {
  margin-top: 30px;
}
.subcollections__list {
  display: flex;
  margin: 20px -20px 0 0;

  @include at-query ( $max, $medium ) {
    display: block;
    margin-right: 0;
  }
}

.subcollection__item {
  display: block;
  width: auto;
  padding-right: 20px;
  position: relative;

  @include at-query ( $min, $large ) {
    padding: 0;
    margin-right: 20px;

    &:last-child {
      margin-right: 0;
    }

    .subcollections__list--3 & { width: calc(33.33% - 20px); }
    .subcollections__list--4 & { width: calc(25% - 20px); }
    .subcollections__list--5 & { width: calc(20% - 20px); }
  }
}
.subcollection__item-image {
  display: block;
  position: relative;
  height: 240px;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  overflow: hidden;

  @include at-query ( $max, $medium ) {
    width: 200px;
    height: 0;
    padding-top: 100%;
  }

  &::before,
  &::after {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, .25);
    transition: background .5s ease;
  }

  &::after {
    left: -100%;
    right: auto;
    width: 100%;
    background: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, .25) 50%, rgba(255, 255, 255, 0) 100%);
  }

  &:hover {
    &::before {
      background: rgba(0, 0, 0, .15);
    }

    &::after {
      animation: move 1s ease;
    }

    .subcollection__item-title span::after {
      width: 100%;
      transition: width .5s ease-out;
    }
  }
}
.subcollection__item-title {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 1;
  max-width: 100%;
  padding: 20px;
  margin: 0;
  color: #fff;

  span {
    display: block;
    position: relative;

    &::after {
      content: '';
      position: absolute;
      top: 100%;
      left: 0;
      width: 0;
      height: 1px;
      background: #fff;
      transition: width .25s ease-in;
    }
  }
}

@keyframes move {
  0% {
    left: -100%;
  }

  100% {
    left: 100%;
  }
}

/* Collection filters */
.collection-section {
  position: relative;
}

.collection__wrapper {
  @include transition( height, 1s, ease-out );

  &::after {
    content: '';
    display: table;
    clear: both;
    line-height: 0;
    height: 0;
  }
}
.collection__filters {
  position: sticky;
  position: -webkit-sticky;
  top: 0;
  z-index: 200;
}

.collection__filters-wrapper {
  position: relative;
  width: 100%;
  padding: 20px 0;
  background: $colorBody;

  // Set white if body background is set to transparent
  &::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: -1;
  }
}

.collection__filter-tag--default {
  margin-bottom: 10px;
  padding-right: 15px;

  button {
    display: block;
    padding-left: 25px;
    position: relative;
    text-align: left;

    &::before {
      content: '';
      position: absolute;
      left: 0;
      top: 50%;
      margin-top: -8px;
      width: 16px;
      height: 16px;
      border: 1px solid $colorBorder;
      background: #fff;
    }
  }

  &.is-active {
    button {
      &::before {
        content: '';
        color: #fff;
        border-color: #333;
        background: #333 url("{{ 'icon-check.svg' | asset_url }}") no-repeat center center;
        background-size: 10px auto;
      }
    }
  }
}

.collection__filter-tag--swatch.swatch {
  &.is-active {
    .swatch-element {
      label {
        &::after {
          opacity: 1;
          visibility: visible;
        }
      }
    }
  }

  .swatch-element {
    width: 60px;
    flex-direction: column;
    align-items: center;
    margin: 0 2px 10px;
    line-height: 1.2;

    label {
      float: none;
      display: block;
      width: {{ swatch_width }};
      position: relative;
      padding: 0;
      margin: 0 auto 5px;
    }

    &::after {
      position: static;
      margin-top: 4px;
      background-color: transparent;
      padding: 0;
      border-radius: 0;
      box-shadow: none;
      border: 0;
      opacity: 1;
      visibility: visible;
      font-size: 11px;
      line-height: 1.2;
      white-space: normal;
      @include transform( 'none' );
    }
  }
}

.collection__filter-tag--number {
  display: inline-block;
  padding: 0;
  margin: 0 6px 10px 0;

  button {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 6px 3px;
    height: 31px;
    min-width: 31px;
    border-radius: 16px;
    line-height: 17px;
    border: 1px solid $colorBorder;
    margin: 0;
  }

  &.is-active {
    button {
      color: #fff;
      border-color: #333;
      background: #333;
    }
  }
}

/* Collection filters top & mobile layout */
.collection__filters {
  // height: 45px;

  h3 {
    padding-top: 5px;
    padding-bottom: 5px;
    margin-bottom: 0;
  }

  .collection__filters-wrapper {
    padding: 0;
    background: $colorBody;
    border-bottom: 1px solid $colorBorder;
  }

  .collection__filter {
    position: static;
    margin-right: 50px;
    @include transition( opacity, .3s, ease );

    @include at-query ( $max, $small ) {
      margin-right: 30px;
    }

    &:last-child {
      margin-right: 0;
    }

    &.is-expanded {
      .collection__filter-tag {
        animation: .3s ease-in-out both fade-in;

        @for $i from 1 through 100 {
          &:nth-child(#{$i}) {
             animation-delay: $i * (.03s);
          }
        }
      }
      .collection__filter-tags {
        opacity: 1;
        visibility: visible;
        margin-top: 1px;
        transition: opacity .3s ease, margin .3s ease, visibility 0s linear 0s;
      }

      .collection__filter-title {
        &::before {
          transform: rotate(270deg);
        }
      }
    }
  }

  &.has-tags-expanded {
    .collection__filter:not(.is-expanded) {
      opacity: .5;
    }
  }

  .collection__filter-title {
    padding-right: 25px;
    position: relative;
    cursor: pointer;

    &::before {
      content: "\e900";
      font-weight: bolder !important;
      font-family: 'custom-icons';
      position: absolute;
      top: 0;
      right: 0;
      transform: rotate(90deg);
      font-size: 80%;
      line-height: 1;
      margin: 5px 5px 5px 15px;
      transition: opacity .3s ease;
    }
  }

  .collection__filters-list {
    padding: 10px 0;
  }

  .collection__filter-tags {
    position: absolute;
    top: 100%;
    right: 0;
    left: 0;
    margin: 11px 0 0;
    width: 100%;
    list-style: none;
    opacity: 0;
    visibility: hidden;
    transition: opacity .3s ease, margin .3s ease, visibility 0s linear .3s;
    background: $colorBody;
    padding: 10px 50px 0;
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    border-bottom: 1px solid $colorBorder;

    @include at-query ( $max, $medium ) {
      display: block;
      width: auto;
      padding: 15px 20px 10px;
      overflow: auto;
      -webkit-overflow-scrolling: touch;
      max-height: calc(100vh - 42px);
      text-align: center;
    }
  }

  .reset-filters.is-visible ~ .collection__filter .collection__filter-tags {
    max-height: calc(100vh - 72px);
  }

  .collection__filter-tag--swatch.swatch {
    margin: 0;

    .swatch-element {
      span {
        display: block;
        border-radius: 50%;
      }
    }

    @include at-query ( $max, $medium ) {
      display: inline-block;
      margin: 0;
    }
  }

  @include at-query ( $min, $large ) {
    .collection__filter-tag--number + .collection__filter-tag--default {
      margin-left: 20px;
    }

    .collection__filter-tag--number ~ .collection__filter-tag--default {
      margin-top: 5px;
    }
  }
}

.collection__filters--has-tags-selected {
  @include at-query ( $max, $postSmall  ) {
    // height: 72px;
  }
}

.collection__filter {
  position: relative;

  h3 {
    position: relative;
    padding-right: 30px;

    .custom-icon-arrow-right {
      position: absolute;
      top: 0;
      right: 0;
      font-size: 80%;
      line-height: 1;
      margin: 8px 0 0;
      @include transform( rotate(90deg) );
    }
  }

  button {
    @include size-body(14px);
    font-family: $font-stack-body;
    font-weight: $font-weight-body;
    font-style: $font-style-body;
    color: inherit;
  }
}

.collection__products {
  position: relative;

  .breadcrumbs {
    float: left;
    margin: 20px 0;

    @include at-query ( $max, $medium ) {
      float: none;
      text-align: center;
    }
  }

  .selector-wrapper {
    padding-right: 0;

    &::after {
      right: 10px;
    }
  }

  .sort-me {
    z-index: 2;
  }

  &::before {
    content: '';
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 300;
    opacity: 0;
    visibility: hidden;
    display: block;
    width: 100%;
    height: 100%;
    background: rgba(255, 255, 255, .65) url( "{{ 'loading.svg' | asset_url }}" ) no-repeat center center;
    background-size: 64px auto;
    transition: opacity .3s ease, visibility 0s linear .3s;
  }

  .is-loading & {
    &::before {
      opacity: 1;
      visibility: visible;
      transition: opacity .3s ease, visibility 0s linear 0s;
    }
  }

  .no-matches {
    @include size-body(16px);
  }
}

.reset-filters {
  display: inline-block;
  margin: 0;
  font-family: $font-stack-body;
  font-weight: $font-weight-body;
  font-style: $font-style-body;
  color: inherit;
  text-align: left;
  @include size-body(14px);
  line-height: 18px;

  .icon {
    float: left;
    margin: -4px 2px -4px -5px;
    width: 26px;
    height: 26px;
    @include transition( transform, .3s, ease );
  }

  &:hover {
    .icon {
      transform: rotate(90deg);
    }
  }
}

.collection__filters-list {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;

  .reset-filters {
    margin-right: 50px;
  }

  @include at-query ( $max, $medium ) {
    justify-content: center;

    .reset-filters {
      width: 100%;
      margin: 0 0 10px;
      text-align: center;

      .icon {
        display: inline-block;
        float: none;
        vertical-align: middle;
      }
    }
  }
}

.collection__filters--left {
  @include at-query ( $min, $largest ) {
    float: left;
    width: 300px;
    height: auto;

    & ~ .collection__products {
      float: right;
      width: calc(100% - 300px);
      min-height: 100vh;

      .is-loading & {
        height: auto;
      }
    }

    .collection__filters-wrapper {
      border: 0;
      background-color: transparent;
      padding-top: 10px;
      left: 0;
      max-height: 100vh;
      overflow-x: hidden;
      overflow-y: auto;
    }

    &.has-tags-expanded .collection__filter:not(.is-expanded) { opacity: 1; }

    .collection__filters-list {
      padding: 10px 0;
      justify-content: flex-start;
      flex-direction: column;
      width: 200px;

      .reset-filters {
        margin: 0 0 15px;
      }
    }

    .collection__filter {
      margin-bottom: 40px;
      margin-right: 0;

      &:last-child {
        margin-bottom: 0;
      }
    }

    .collection__filter-title {
      display: block;
      border-bottom: 1px solid $colorBorder;
      padding-bottom: 5px;
      padding-right: 0;
      margin-bottom: 15px;
      cursor: default;
      text-transform: uppercase;
      letter-spacing: 0.1em;
      @include size-body(12px);

      &::before,
      &::after {
        content: none;
      }
    }

    .collection__filter-tag {
      transition: none;
      opacity: 1;
    }
    .collection__filter-tags {
      list-style: none;
      margin: 0;
      padding: 0;
      position: static;
      opacity: 1;
      visibility: visible;
      background-color: transparent;
      display: block;
      border-bottom: 0;

      &::before {
        content: none;
      }
    }

    .collection__filter-tag--swatch.swatch {
      margin: 0 7px 9px 0;
      display: inline-block;
      vertical-align: top;

      .swatch-element {
        margin: 0;
        display: block;
        width: auto;

        &::after {
          position: absolute;
          top: 100%;
          background: $colorBody;
          color: $colorPrimary;
          padding: 2px 5px;
          border-radius: 3px;
          box-shadow: 0 1px 3px rgba(0, 0, 0, .15);
          border: 1px solid $colorBorder;
          white-space: nowrap;
          opacity: 0;
          visibility: hidden;
          transition: none;
          @include size-body(11px);
          @include transform(translateY(50%));

          .supports-touch & {
            content: none;
          }
        }

        &:hover {
          z-index: 5;
          &::after {
            opacity: 1;
            visibility: visible;
            transition: transform .3s ease, opacity .3s ease, visibility 0s linear 0s;
            @include transform(translateY(0));
          }
        }
      }
    }

    .collection__filter-tag--number + .collection__filter-tag--default {
      margin-left: 0;
      margin-top: 5px;
    }
  }
}

// Filter tags animation
@keyframes fade-in {
  0% {
    opacity: 0;
    transform: scale(0);
  }

  100% {
    opacity: 1;
    transform: scale(1);
  }
}

/* overlay at start */
.mfp-fade.mfp-bg {
  opacity: 0;

  -webkit-transition: all 0.15s ease-out;
  -moz-transition: all 0.15s ease-out;
  transition: all 0.15s ease-out;
}
/* overlay animate in */
.mfp-fade.mfp-bg.mfp-ready {
  opacity: 0.8;
}
/* overlay animate out */
.mfp-fade.mfp-bg.mfp-removing {
  opacity: 0;
}

/* content at start */
.mfp-fade.mfp-wrap .mfp-content {
  opacity: 0;

  -webkit-transition: all 0.15s ease-out;
  -moz-transition: all 0.15s ease-out;
  transition: all 0.15s ease-out;
}
/* content animate it */
.mfp-fade.mfp-wrap.mfp-ready .mfp-content {
  opacity: 1;
}
/* content animate out */
.mfp-fade.mfp-wrap.mfp-removing .mfp-content {
  opacity: 0;
}
